@article{ ISI:000454626800002,
Author = {Li, Chaojun and Wang, Shijie and Bai, Xiaoyong and Tan, Qiu and Yang,
   Yujie and Li, Qin and Wu, Luhua and Xiao, Jianyong and Qian, Qinghuan
   and Chen, Fei and Li, Huiwen and Cao, Yue and Wang, Mingming and Wang,
