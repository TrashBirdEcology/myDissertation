@article{ ISI:000453768800010,
Author = {Barnard, David M. and Knowles, John F. and Barnard, Holly R. and
   Goulden, Michael L. and Hu, Jia and Litvak, Marcy E. and Molotch, Noah
   P.},
Title = {{Reevaluating growing season length controls on net ecosystem production
   in evergreen conifer forests}},
Journal = {{SCIENTIFIC REPORTS}},
