@incollection{ ISI:000283524600001,
Author = {Lopez-Hernandez, Danilo},
Editor = {{Rojas, N and Prieto, R}},
Title = {{AGRICULTURAL SYSTEMS IN THE SAVANNA-FOREST ECOTONE OF VENEZUELAN
   AMAZONIAN: EVALUATION OF SOIL QUALITY INDICATORS}},
Booktitle = {{AMAZON BASIN: PLANT LIFE, WILDLIFE AND ENVIRONMENT}},
Series = {{Environmental Research Advances}},
Year = {{2010}},
Pages = {{1-40}},
Abstract = {{Puerto Ayacucho (PA), the capital of Amazonas State in Venezuela, has
   experienced a rapid population growth during the last decades due to the
   migration of indigenous communities living in the tropical rain forest
   who have settled in the nearby savanna, along the main highway. This
   poses the question of how to feed the growing population given the
   existing restrictions of low-fertility soils, risk of erosion,
   decreasing biodiversity and changes of consumption habits of the
   indigenous population due to transculturation. A variety of agrosystems
   (AS) ranging from the traditional conuco used by the indigenous
   communities to the agroforestry being practiced in the savanna by
   criollos have been identified in the forest-savanna ecotone surrounding
   PA. Those AS were evaluated by using criteria of sustainability; the
   main emphasis pointed to ecological and economical parameters.
   The Traditional Conuco (TC) could be considered economically and
   ecologically sustainable as long as there is no population growth and
   there is enough forest land to open a new ``conuco{''} and allow the old
   one to rest for a period longer than twenty years. Since there is not
   enough forest land available in nearby PA, the TC is unsustainable in
   this area.
   The conuco for Surplus (CS) and the Improved Conuco (IC) are not only
   unable to feed a growing population, but also contribute further than
   the TC to deforestation and associated erosion as it is occurring in the
   adjacent forests of PA. Nonetheless, the worst situation in terms of
   economical and ecological unsustainability is the case of the Conuco in
   Fallow (CF).
   In contrast to the conucos, there is a successful adaptation of
   agroforestry systems (AFS) in sandy savanna ultisols and entisols by
   amelioration of both soil structure and fertility when animal manure is
   used. Nonetheless, the extensive implementation of organic agroforestal
   systems in the area is, at the present time, limited due to the high
   price of animal food. Therefore, government policies addressed to
   achieve sustainable agriculture should consider the possibility of
   stimulating the AFS after identifying the best condition under which the
   indigenous as well as the criollo could adopt alternative AS such AFS
   without cultural or social costs.
   For the different conucos analyzed, although significant changes were
   found for several soil quality indicators, the changes obtained in many
   cases were not so evident, particularly in the case of the conucos in
   fallow; however, enzymatic activities (phosphatase and urease) were
   revealed as a very useful tool for analyzing changes after conuco
   establishment. On the other hand, the use of organic waste as
   alternative soil fertility amendments in nutrient sandy depleted savanna
   soils of Venezuelan Amazonia have resulted in increased organic matter
   and biological and enzymatic activities in soils which, in turn, may be
   related to crop productivity.}},
ISBN = {{978-1-60741-463-6}},
Unique-ID = {{ISI:000283524600001}},
}

@article{ ISI:000272372200013,
Author = {Hadfield, Jarrod D. and Wilson, Alastair J. and Garant, Dany and
   Sheldon, Ben C. and Kruuk, Loeske E. B.},
Title = {{The Misuse of BLUP in Ecology and Evolution}},
Journal = {{AMERICAN NATURALIST}},
Year = {{2010}},
Volume = {{175}},
Number = {{1}},
Pages = {{116-125}},
Month = {{JAN}},
Abstract = {{Best linear unbiased prediction (BLUP) is a method for obtaining point
   estimates of a random effect in a mixed effect model. Over the past
   decade it has been used extensively in ecology and evolutionary biology
   to predict individual breeding values and reaction norms. These
   predictions have been used to infer natural selection, evolutionary
   change, spatial-genetic patterns, individual reaction norms, and
   frailties. In this article we show analytically and through simulation
   and example why BLUP often gives anticonservative and biased estimates
   of evolutionary and ecological parameters. Although some concerns with
   BLUP methodology have been voiced before, the scale and breadth of the
   problems have probably not been widely appreciated. Bias arises because
   BLUPs are often used to estimate effects that are not explicitly
   accounted for in the model used to make the predictions. In these cases,
   predicted breeding values will often say more about phenotypic patterns
   than the genetic patterns of interest. An additional problem is that
   BLUPs are point estimates of quantities that are usually known with
   little certainty. Failure to account for this uncertainty in subsequent
   tests can lead to both bias and extreme anticonservatism. We demonstrate
   that restricted maximum likelihood and Bayesian solutions exist for
   these problems and show how unbiased and powerful tests can be derived
   that adequately quantify uncertainty. Of particular utility is a new
   test for detecting evolutionary change that not only accounts for
   prediction error in breeding values but also accounts for drift. To
   illustrate the problem, we apply these tests to long-term data on the
   Soay sheep (Ovis aries) and the great tit (Parus major) and show that
   previously reported temporal trends in breeding values are not
   supported.}},
DOI = {{10.1086/648604}},
ISSN = {{0003-0147}},
EISSN = {{1537-5323}},
ResearcherID-Numbers = {{Sheldon, Ben/A-8056-2010
   Wilson, Alastair/F-3118-2010
   Garant, Dany/D-7406-2013
   Kruuk, Loeske/J-3295-2012
   }},
ORCID-Numbers = {{Sheldon, Ben/0000-0002-5240-7828
   Garant, Dany/0000-0002-8091-1044
   Wilson, Alastair/0000-0002-5045-2051}},
Unique-ID = {{ISI:000272372200013}},
}

@incollection{ ISI:000284978200008,
Author = {Dickinson, Janis L. and Zuckerberg, Benjamin and Bonter, David N.},
Editor = {{Futuyma, DJ and Shafer, HB and Simberloff, D}},
Title = {{Citizen Science as an Ecological Research Tool: Challenges and Benefits}},
Booktitle = {{ANNUAL REVIEW OF ECOLOGY, EVOLUTION, AND SYSTEMATICS, VOL 41}},
Series = {{Annual Review of Ecology Evolution and Systematics}},
Year = {{2010}},
Volume = {{41}},
Pages = {{149-172}},
Abstract = {{Citizen science, the involvement of volunteers in research, has
   increased the scale of ecological field studies with continent-wide,
   centralized monitoring efforts and, more rarely, tapping of volunteers
   to conduct large, coordinated, field experiments The unique benefit for
   the field of ecology lies in understanding processes occurring at broad
   geographic scales and on private lands, which are impossible to sample
   extensively with traditional field research models Citizen science
   produces large, longitudinal data sets, whose potential for error and
   bias is poorly understood Because it does not usually aim to uncover
   mechanisms underlying ecological patterns, citizen science is best
   viewed as complementary to more localized, hypothesis-driven research In
   the process of addressing the impacts of current, global
   ``experiments{''} altering habitat and climate, large-scale citizen
   science has led to new, quantitative approaches to emerging questions
   about the distribution and abundance of organisms across space and time}},
DOI = {{10.1146/annurev-ecolsys-102209-144636}},
