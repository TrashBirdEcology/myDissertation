Unique-ID = {{ISI:000296754600005}},
}

@article{ ISI:000297089500013,
Author = {Hashimoto, S. and Wattenbach, M. and Smith, P.},
Title = {{A new scheme for initializing process-based ecosystem models by scaling
   soil carbon pools}},
Journal = {{ECOLOGICAL MODELLING}},
Year = {{2011}},
Volume = {{222}},
Number = {{19}},
Pages = {{3598-3602}},
Month = {{OCT 10}},
Abstract = {{Process-based ecosystem models are useful tools, not only for
   understanding the forest carbon cycle, but also for predicting future
   change. In order to apply a model to simulate a specific time period,
   model initialization is required. In this study, we propose a new scheme
   of initialization for forest ecosystem models, which we term a
   ``slow-relaxation scheme{''}, that entails scaling of the soil carbon
   and nitrogen pools slowly during the spin-up period. The proposed
   slow-relation scheme was tested with the CENTURY version 4 ecosystem
   model. Three different combinations of scaled soil pools were also
   tested, and compared to the results from a fast-relaxation regime. The
   fast-relaxation of soil pools produced unstable, transient model
   behaviour whereas slow-relaxation overcame this instability. This
   approach holds promise for initializing ecosystem models, and for
   starting simulations with more realistic initial conditions. (C) 2011
   Elsevier B.V. All rights reserved.}},
DOI = {{10.1016/j.ecolmodel.2011.08.011}},
ISSN = {{0304-3800}},
ResearcherID-Numbers = {{Wattenbach, Martin/D-1482-2013
   Smith, Pete/G-1041-2010
   Hashimoto, Shoji/C-5785-2011}},
ORCID-Numbers = {{Wattenbach, Martin/0000-0002-1843-1031
