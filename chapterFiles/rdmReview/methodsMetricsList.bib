@article{fath_regime_2003,
	title = {Regime changes in ecological systems: an information theory approach},
	volume = {222},
	issn = {0022-5193},
	shorttitle = {Regime changes in ecological systems},
	url = {http://www.sciencedirect.com/science/article/pii/S0022519303000675},
	doi = {10.1016/S0022-5193(03)00067-5},
	abstract = {We present our efforts at developing an ecological system index using information theory. Specifically, we derive an expression for Fisher Information based on sampling of the system trajectory as it evolves in the space defined by the state variables of the system, i.e. its state space. The Fisher Information index, as we have derived it, is a measure of system order, and captures the characteristic variation in speed and acceleration along the system's periodic steady-state trajectories. When calculated repeatedly over the system period, this index tracks steady states and transient behavior. We believe that such an index could be useful in detecting system ???flips??? associated with a regime change, i.e. determining when systems are in a transient between one steady state and another. We illustrate the concepts using model ecosystems.},
	number = {4},
	urldate = {2017-03-20},
	journal = {Journal of Theoretical Biology},
	author = {Fath, Brian D. and Cabezas, Heriberto and Pawlowski, Christopher W.},
	month = jun,
	year = {2003},
	note = {00098},
	keywords = {Fisher information, Ecological models, Predator???prey models},
	pages = {517--530},
	file = {ScienceDirect Full Text PDF:/Users/jessicaburnett/Zotero/storage/VXIZDMQ2/Fath et al. - 2003 - Regime changes in ecological systems an informati.pdf:application/pdf}
}


@article{solow_test_2005,
	title = {A test for a regime shift},
	volume = {14},
	issn = {1365-2419},
