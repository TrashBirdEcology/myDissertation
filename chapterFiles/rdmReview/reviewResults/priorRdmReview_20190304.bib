	keywords = {Facilitation, Competition, Hysteresis, Restoration, Ecosystem functioning, Positive feedback, State transition},
	pages = {991--998},
	file = {ScienceDirect Full Text PDF:/Users/jessicaburnett/Zotero/storage/BJ59TUYR/Kadowaki et al. - 2018 - Merging community assembly into the regime-shift a.pdf:application/pdf;ScienceDirect Snapshot:/Users/jessicaburnett/Zotero/storage/BLZTYMBN/S1470160X17307471.html:text/html}
}

@article{milkoreit_defining_2018,
	title = {Defining tipping points for social-ecological systems scholarship – an interdisciplinary literature review},
	issn = {1748-9326},
	url = {http://iopscience.iop.org/10.1088/1748-9326/aaaa75},
	doi = {10.1088/1748-9326/aaaa75},
	abstract = {Abstract The term tipping point has experienced explosive popularity across multiple disciplines over the last decade. Research on social-ecological systems (SES) has contributed to the growth and diversity of the term’s use. The diverse uses of the term obscure potential differences between tipping behavior in natural and social systems, and issues of causality across natural and social system components in SES. This paper aims to create the foundation for a discussion within the SES research community about the appropriate use of the term tipping point, especially the relatively novel term ‘social tipping point.’ We review existing literature on tipping points and similar concepts (e.g., regime shifts, critical transitions) across all spheres of science published between 1960 and 2016 with a special focus on a recent and still small body of work on social tipping points. We combine quantitative and qualitative analyses in a bibliometric approach, rooted in an expert elicitation process. We find that the term tipping point became popular after the year 2000 – long after the terms regime shift and critical transition - across all spheres of science. We identify 23 distinct features of tipping point definitions and their prevalence across disciplines, but find no clear taxonomy of discipline-specific definitions. Building on the most frequently used features, we propose definitions for tipping points in general and social tipping points in SES in particular.},
	language = {en},
	urldate = {2018-02-01},
	journal = {Environmental Research Letters},
	author = {Milkoreit, Manjana and Hodbod, Jennifer and Baggio, Jacopo and Benessaiah, Karina and Contreras, Rafael Calderon and Donges, Jonathon F. and Mathias, Jean-Denis and Rocha, Juan Carlos and Schoon, Michael and Werners, Saskia},
	year = {2018},
	file = {IOP Full Text PDF:/Users/jessicaburnett/Zotero/storage/EA4CC2GW/Milkoreit et al. - 2018 - Defining tipping points for social-ecological syst.pdf:application/pdf}
}

