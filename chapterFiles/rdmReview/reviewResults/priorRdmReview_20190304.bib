@article{fritz_critical_2005,
	title = {A {Critical} {Review} of the {Regime} {Shift}-“{Junk} {Food}”-{Nutritional} {Stress} {Hypothesis} for the {Decline} of the {Western} {Stock} of {Steller} {Sea} {Lion}},
	volume = {21},
	issn = {1748-7692},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1748-7692.2005.tb01245.x/abstract},
	doi = {10.1111/j.1748-7692.2005.tb01245.x},
	abstract = {Steller sea lions (Eumetopias jubatus) in the central and western Gulf of Alaska, Aleutian Islands, and Bering Sea have declined by 80\% in the last 30 yr. One hypothesis for the decline in this western Steller sea lion population is that a climate regime shift in 1976–1977 changed the species composition of the fish community and reduced the nutritional quality (energy density) of the sea lion prey field. This in turn led to nutritional stress and reduced individual fitness, survival, and reproduction of sea lions. Implications of this regime shift-“junk food” hypothesis are that (1) the recruitment and abundance of supposed high quality species (e.g., Pacific herring, Clupea pallasi) decreased while those of supposed low quality (e.g., species in the family Gadidae) increased following the regime shift, (2) Steller sea lion diets shifted in response to this change in fish community structure, and (3) a diet composed principally of gadids (e.g., walleye pollock, Theragra chalcogramma) is detrimental to sea lion fitness and survival. We examine data relating to each of these implications and find little support for the hypothesis that increases in the availability and consumption of gadids following the regime shift are primarily responsible for the decline of the western population of Steller sea lion.},
	language = {en},
	number = {3},
	urldate = {2017-12-07},
	journal = {Marine Mammal Science},
	author = {Fritz, Lowell W. and Hinckley, Sarah},
	month = jul,
	year = {2005},
	keywords = {Regime shifts, diet, energy density, Eumetopias jubatus, nutritional stress, population decline, prey quality, Steller sea lions},
	pages = {476--518},
	file = {Full Text PDF:/Users/jessicaburnett/Zotero/storage/NY7A6WPD/Fritz and Hinckley - 2005 - A Critical Review of the Regime Shift-“Junk Food”-.pdf:application/pdf;Snapshot:/Users/jessicaburnett/Zotero/storage/RGJCD6V5/abstract.html:text/html}
}

@article{ahmad_using_2016,
	title = {Using {Fisher} information to track stability in multivariate systems},
	volume = {3},
	copyright = {© 2016 The Authors.. Published by the Royal Society under the terms of the Creative Commons Attribution License http://creativecommons.org/licenses/by/4.0/, which permits unrestricted use, provided the original author and source are credited.},
