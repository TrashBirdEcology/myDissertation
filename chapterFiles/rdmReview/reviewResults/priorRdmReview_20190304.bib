	author = {Kefi, Sonia and Guttal, Vishwesha and Brock, William A. and Carpenter, Stephen R. and Ellison, Aaron M. and Livina, Valerie N. and Seekell, David A. and Scheffer, Marten and van Nes, Egbert H. and Dakos, Vasilis},
	month = mar,
