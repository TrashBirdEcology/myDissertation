}

@article{chen_analyzing_2016,
	title = {Analyzing the complexity of cone production in longleaf pine by multiscale entropy},
	volume = {35},
	issn = {1054-9811},
	doi = {10.1080/10549811.2015.1135294},
	abstract = {The longleaf pine (Pinus palustris Mill.) forests are important ecosystems in the southeastern USA because of their ecological and economic value. Since European settlement, longleaf pine ecosystems have dramatically declined in extent, to the degree that they are now listed as endangered ecosystems. Its sporadic seed production, which limits the frequency of natural regeneration, is identified as a significant factor in this decline. Previous studies did not characterize the complexity in cone production. Here a method of multiscale entropy is used to analyze long-term data for cone production in longleaf pine forests at six sites across its native range. Our results indicate that there exists a regime shift for cone production at each site. The corresponding time scales of the regime shift are generally 1-9, 10-12, 13-16, and 17-24 yr. Overall patterns for the complexity of cone production with the change of time scale are similar among sites, with exception of the Red Hills (FL). There are high correlations between entropy of cone production and entropy of annual mean air temperature and annual total precipitation at all sites. These results provide new insight into the complexity of cone production of longleaf pine forests with significant management implications.},
	language = {English},
	number = {2},
	journal = {Journal of Sustainable Forestry},
	author = {Chen, Xiongwen and Guo, Qinfeng and Brockway, Dale G.},
	year = {2016},
	note = {WOS:000373572400005},
	keywords = {regime shift, management, restoration, ecology, ecosystem, Climate, scale},
	pages = {172--182}
}

@article{bestelmeyer_analysis_2011,
	title = {Analysis of abrupt transitions in ecological systems},
	volume = {2},
	issn = {2150-8925},
	url = {http://onlinelibrary.wiley.com/doi/10.1890/ES11-00216.1/abstract},
	doi = {10.1890/ES11-00216.1},
	abstract = {The occurrence and causes of abrupt transitions, thresholds, or regime shifts between ecosystem states are of great concern and the likelihood of such transitions is increasing for many ecological systems. General understanding of abrupt transitions has been advanced by theory, but hindered by the lack of a common, accessible, and data-driven approach to characterizing them. We apply such an approach to 30–60 years of data on environmental drivers, biological responses, and associated evidence from pelagic ocean, coastal benthic, polar marine, and semi-arid grassland ecosystems. Our analyses revealed one case in which the response (krill abundance) linearly tracked abrupt changes in the driver (Pacific Decadal Oscillation), but abrupt transitions detected in the three other cases (sea cucumber abundance, penguin abundance, and black grama grass production) exhibited hysteretic relationships with drivers (wave intensity, sea-ice duration, and amounts of monsoonal rainfall, respectively) through a variety of response mechanisms. The use of a common approach across these case studies illustrates that: the utility of leading indicators is often limited and can depend on the abruptness of a transition relative to the lifespan of responsive organisms and observation intervals; information on spatiotemporal context is useful for comparing transitions; and ancillary information from associated experiments and observations aids interpretation of response-driver relationships. The understanding of abrupt transitions offered by this approach provides information that can be used to manage state changes and underscores the utility of long-term observations in multiple sentinel sites across a variety of ecosystems.},
	language = {en},
	number = {12},
	urldate = {2018-01-22},
	journal = {Ecosphere},
	author = {Bestelmeyer, Brandon T. and Ellison, Aaron M. and Fraser, William R. and Gorman, Kristen B. and Holbrook, Sally J. and Laney, Christine M. and Ohman, Mark D. and Peters, Debra P. C. and Pillsbury, Finn C. and Rassweiler, Andrew and Schmitt, Russell J. and Sharma, Sapna},
	month = dec,
	year = {2011},
	keywords = {regime shifts, thresholds, leading indicators, alternative states, Bouteloua eriopoda, desert grassland, krill, Nyctiphanes simplex, Pachythyone rubra, penguins, Pygoscelis, sea cucumbers},
	pages = {1--26},
	file = {Full Text PDF:/Users/jessicaburnett/Zotero/storage/ND7YTD3Y/Bestelmeyer et al. - 2011 - Analysis of abrupt transitions in ecological syste.pdf:application/pdf;Full Text PDF:/Users/jessicaburnett/Zotero/storage/BKGWTVV5/Bestelmeyer et al. - 2011 - Analysis of abrupt transitions in ecological syste.pdf:application/pdf;Full Text PDF:/Users/jessicaburnett/Zotero/storage/IGDUH2QP/Bestelmeyer et al. - 2011 - Analysis of abrupt transitions in ecological syste.pdf:application/pdf;Snapshot:/Users/jessicaburnett/Zotero/storage/HDZLN3PW/abstract.html:text/html;Snapshot:/Users/jessicaburnett/Zotero/storage/BZ2EUXXS/abstract.html:text/html;Snapshot:/Users/jessicaburnett/Zotero/storage/F87ZH2NZ/abstract.html:text/html}
