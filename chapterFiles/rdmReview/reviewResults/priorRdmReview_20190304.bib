	pmid = {26216946},
	note = {00000 },
	keywords = {early warning signals, environmental drivers, population collapse, stability–resilience relation},
	pages = {10056--10061},
	file = {Full Text PDF:/Users/jessicaburnett/Zotero/storage/DXSCCPLQ/Dai et al. - 2015 - Relation between stability and resilience determin.pdf:application/pdf;Full Text PDF:/Users/jessicaburnett/Zotero/storage/WNNXKIV3/Dai et al. - 2015 - Relation between stability and resilience determin.pdf:application/pdf;Snapshot:/Users/jessicaburnett/Zotero/storage/6KGER884/10056.html:text/html;Snapshot:/Users/jessicaburnett/Zotero/storage/3QYFCZYT/10056.html:text/html}
