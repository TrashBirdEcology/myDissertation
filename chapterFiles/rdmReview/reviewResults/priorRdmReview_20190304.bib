	month = jul,
	year = {2018},
	keywords = {disturbance, resilience, regime shifts, collapse, tipping points, stochasticity},
	pages = {513--526},
	file = {ScienceDirect Full Text PDF:/Users/jessicaburnett/Zotero/storage/B7XLSBU5/Ratajczak et al. - 2018 - Abrupt Change in Ecological Systems Inference and.pdf:application/pdf;ScienceDirect Snapshot:/Users/jessicaburnett/Zotero/storage/C7NNAS23/S0169534718300910.html:text/html}
}

@article{ritchie_probability_2017,
