	keywords = {resilience, regime shift, early warning, state-and-transition model, spatial regimes, rangeland monitoring},
	file = {ScienceDirect Snapshot:/Users/jessicaburnett/Zotero/storage/E5INWGUE/S1550742418301337.html:text/html}
}

