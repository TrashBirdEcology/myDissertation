}

@article{zhao_analysis_2015,
	title = {Analysis of temporal and spatial trends of hydro-climatic variables in the {Wei} {River} {Basin}},
	volume = {139},
	issn = {0013-9351},
	url = {http://www.sciencedirect.com/science/article/pii/S0013935114004794},
	doi = {https://doi.org/10.1016/j.envres.2014.12.028},
	abstract = {The Wei River is the largest tributary of the Yellow River in China. The relationship between runoff and precipitation in the Wei River Basin has been changed due to the changing climate and increasingly intensified human activities. In this paper, we determine abrupt changes in hydro-climatic variables and identify the main driving factors for the changes in the Wei River Basin. The nature of the changes is analysed based on data collected at twenty-one weather stations and five hydrological stations in the period of 1960–2010. The sequential Mann–Kendall test analysis is used to capture temporal trends and abrupt changes in the five sub-catchments of the Wei River Basin. A non-parametric trend test at the basin scale for annual data shows a decreasing trend of precipitation and runoff over the past fifty-one years. The temperature exhibits an increase trend in the entire period. The potential evaporation was calculated based on the Penman-Monteith equation, presenting an increasing trend of evaporation since 1990. The stations with a significant decreasing trend in annual runoff mainly are located in the west of the Wei River primarily interfered by human activities. Regression analysis indicates that human activity was possibly the main cause of the decline of runoff after 1970.},
	journal = {Environmental Research},
	author = {Zhao, Jing and Huang, Qiang and Chang, Jianxia and Liu, Dengfeng and Huang, Shengzhi and Shi, Xiaoyu},
	year = {2015},
	keywords = {Change points, Precipitation, The Mann–Kendall test method, The Wei River Basin, Trends},
	pages = {55 -- 64}
}

@article{chen_abrupt_2014,
	title = {Abrupt spatiotemporal land and water changes and their potential drivers in {Poyang} {Lake}, 2000–2012},
	volume = {98},
	issn = {0924-2716},
	url = {http://www.sciencedirect.com/science/article/pii/S0924271614002391},
	doi = {https://doi.org/10.1016/j.isprsjprs.2014.09.014},
	abstract = {Driven by various natural and anthropogenic factors, Poyang Lake, the largest freshwater lake in China, has experienced significant land use/cover changes in the past few decades. The aim of this study is to investigate the spatial–temporal patterns of abrupt changes and detect their potential drivers in Poyang Lake, using time-series Moderate Resolution Imaging Spectroradiometer (MODIS) 16-day maximum value composite vegetation indices between 2000 and 2012. The breaks for additive seasonal and trend (BFAST) method was applied to the smoothed time-series normalized difference vegetation index (NDVI), to detect the timing and magnitude of abrupt changes in the trend component. Large part of Poyang Lake (98.9\% for trend component) has experienced abrupt changes in the past 13years, and the change patterns, including the distributions in timing and magnitudes of major abrupt trend changes between water bodies and land areas were clearly differentiated. Most water bodies had abrupt increasing NDVI changes between 2010 and 2011, caused by the sequential severe flooding and drought in the two years. In contrast, large parts of the surrounding land areas had abrupt decreasing NDVI changes. Large decreasing changes occurred around 2003 at the city of Nanchang, which were driven by urbanization. These results revealed spatial–temporal land cover changing patterns and potential drivers in the wetland ecosystem of Poyang Lake.},
	journal = {ISPRS Journal of Photogrammetry and Remote Sensing},
	author = {Chen, Lifan and Michishita, Ryo and Xu, Bing},
	year = {2014},
	keywords = {Driving forces, MODIS, Signal decomposition, Time-series analysis, Trend breaks, Vegetation index},
	pages = {85 -- 93}
}

@article{yao_moving_2017,
	title = {Moving window adaptive soft sensor for state shifting process based on weighted supervised latent factor analysis},
	volume = {61},
	issn = {0967-0661},
	url = {http://www.sciencedirect.com/science/article/pii/S0967066117300175},
	doi = {https://doi.org/10.1016/j.conengprac.2017.02.002},
	abstract = {Process nonlinearity and state shifting are two of the main factors that cause poor performance of online soft sensors. Adaptive soft sensor is a common practice to ensure high predictive accuracy. In this paper, the moving window method is introduced to the supervised latent factor analysis model to capture the state shifting feature of the process. To make the moving window strategy more efficient, the weighted form of the supervised latent factor analysis approach is applied. In this method, contributions of training samples are expressed through incorporating the similarity index into the noise variance of the process variable, which renders strong adaptability of the method for describing nonlinear relationships and abrupt changes of the process. A numerical example and a real industrial process are provided to demonstrate the effectiveness of the proposed adaptive soft sensor.},
	journal = {Control Engineering Practice},
	author = {Yao, Le and Ge, Zhiqiang},
	year = {2017},
	keywords = {Adaptive soft sensor, Expectation Maximization algorithm, Moving window, State shifting, Weighted supervised latent factor analysis},
	pages = {72 -- 80}
}

@article{salehpour_-line_2011,
	title = {An {On}-line {Method} for {Estimation} of {Piecewise} {Constant} {Parameters} in {Linear} {Regression} {Models}},
	volume = {44},
	issn = {1474-6670},
	url = {http://www.sciencedirect.com/science/article/pii/S1474667016440991},
