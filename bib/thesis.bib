	journal = {Methods in Ecology and Evolution},
	author = {Borer, Elizabeth T. and Harpole, W. Stanley and Adler, Peter B. and Lind, Eric M. and Orrock, John L. and Seabloom, Eric W. and Smith, Melinda D.},
	month = jan,
	year = {2014},
