	author = {Spanbauer, Trisha L. and Allen, Craig R. and Angeler, David G. and Eason, Tarsha and Fritz, Sherilyn C. and Garmestani, Ahjond S. and Nash, Kirsty L. and Stone, Jeffery R.},
