	note = {00000},
	keywords = {Information, Hierarchies, Maximum power, Networks, Systems ecology, Thermodynamics},
	pages = {112--118},
	file = {ScienceDirect Full Text PDF:/Users/jessicaburnett/Zotero/storage/722872YJ/J??rgensen et al. - 2016 - Recent progress in systems ecology.pdf:application/pdf;ScienceDirect Snapshot:/Users/jessicaburnett/Zotero/storage/YWQ2DGC8/S0304380015003701.html:text/html}
}

