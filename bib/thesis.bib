	author = {Ahmad, Nasir and Derrible, Sybil and Eason, Tarsha and Cabezas, Heriberto},
	month = nov,
	year = {2016},
	note = {00005},
	keywords = {Sustainability, resilience, Fisher information, regime changes, shifts, usa, big data, anthropocene, urban systems, data mining, information science, ohio},
	pages = {160582},
	file = {Full Text PDF:/Users/jessicaburnett/Zotero/storage/UZTK8BMX/Ahmad et al. - 2016 - Using Fisher information to track stability in mul.pdf:application/pdf;Snapshot:/Users/jessicaburnett/Zotero/storage/KIHJJMTK/160582.html:text/html}
