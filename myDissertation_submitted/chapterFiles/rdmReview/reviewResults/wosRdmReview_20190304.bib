ORCID-Numbers = {{Struchiner, Claudio/0000-0003-2114-847X
   Gomes, Gabriela/0000-0002-1454-4979
   Teixeira, Luis/0000-0001-8326-6645
   Souto-Maior, Caetano/0000-0002-0271-2576
   Lopes, Joao/0000-0001-5670-2069
